{
    "editor.fontSize": 17,
    "editor.fontFamily": "Operator Mono Lig",
    "editor.fontLigatures": true,
    "editor.rulers": [
        100
    ],
    "editor.tabCompletion": "on",
    "editor.acceptSuggestionOnEnter": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "workbench.colorTheme": "Dracula",
    "workbench.iconTheme": "material-icon-theme",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.hovers.currentLine.over": "line",
    "newFile.defaultFileExtension": ".rb",
    "newFile.showPathRelativeTo": "project",
    "newFile.relativeTo": "file",
    "vim.useSystemClipboard": true,
    "[ruby]": {
        "editor.tabSize": 2
    },
    "eslint.validate": [
        "javascripta",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    // "[javascript]": {
    //     "editor.formatOnSave": true,
    // },
    // "[javascriptreact]": {
    //     "editor.formatOnSave": true,
    // },
    // "[typescript]": {
    //     "editor.formatOnSave": true,
    // },
    // "[typescriptreact]": {
    //     "editor.formatOnSave": true,
    // },
    "[elixir]": {
        "editor.formatOnSave": true,
    },
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.rulers": [100],
        "editor.tabSize": 4,
    },
    "go.testOnSave": true,
    "rust.show_warnings": false,
    // Disable italics.
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment keyword.codetag.notation",
                    "comment",
                    "comment.block",
                    "comment.block.documentation keyword",
                    "comment.block.documentation storage.type.class",
                    "comment.block.documentation variable",
                    "comment.block.documentation",
                    "comment.line",
                    "constant",
                    "constant.character",
                    "constant.character.escape",
                    "constant.numeric",
                    "constant.numeric.float",
                    "constant.numeric.hex",
                    "constant.numeric.integer",
                    "constant.numeric.octal",
                    "constant.other",
                    "constant.regexp",
                    "constant.rgb-value",
                    "emphasis",
                    "entity",
                    "entity.name",
                    "entity.name.class",
                    "entity.name.function",
                    "entity.name.method",
                    "entity.name.section",
                    "entity.name.selector",
                    "entity.name.tag",
                    "entity.name.type",
                    "entity.name.type",
                    "entity.name.variable.parameter",
                    "entity.other",
                    "entity.other.attribute-name",
                    "entity.other.attribute-name",
                    "entity.other.inherited-class",
                    "invalid",
                    "invalid.deprecated",
                    "invalid.illegal",
                    "keyword",
                    "keyword.control",
                    "keyword.expressions-and-types.swift",
                    "keyword.operator",
                    "keyword.operator.arithmetic",
                    "keyword.operator.assignment",
                    "keyword.operator.logical",
                    "keyword.operator.new",
                    "keyword.other",
                    "keyword.other.this",
                    "keyword.primitive-datatypes.swift",
                    "keyword.type.cs",
                    "markup",
                    "markup.bold",
                    "markup.changed",
                    "markup.deleted",
                    "markup.heading",
                    "markup.inline.raw",
                    "markup.inserted",
                    "markup.italic",
                    "markup.list",
                    "markup.list.numbered",
                    "markup.list.unnumbered",
                    "markup.other",
                    "markup.quote",
                    "markup.raw",
                    "markup.underline",
                    "markup.underline.link",
                    "meta",
                    "meta.at-rule.function variable",
                    "meta.at-rule.mixin variable",
                    "meta.block",
                    "meta.cast",
                    "meta.class",
                    "meta.decorator variable.other.readwrite",
                    "meta.export variable.other.readwrite",
                    "meta.function",
                    "meta.function-call",
                    "meta.function.arguments variable.other.php",
                    "meta.import variable.other.readwrite",
                    "meta.object-binding-pattern-variable variable.object.property",
                    "meta.preprocessor",
                    "meta.protocol-list.objc",
                    "meta.return-type",
                    "meta.return-type.objc",
                    "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql",
                    "meta.selector",
                    "meta.tag",
                    "meta.type",
                    "meta.type.annotation",
                    "meta.variable.assignment.destructured.object.coffee variable",
                    "punctuation.definition.string.begin",
                    "punctuation.definition.string.end",
                    "punctuation.separator",
                    "punctuation.separator.continuation",
                    "punctuation.terminator",
                    "source.go storage.type",
                    "storage",
                    "storage.class.std.rust",
                    "storage.modifier",
                    "storage.type",
                    "storage.type.attribute.swift",
                    "storage.type.c",
                    "storage.type.core.rust",
                    "storage.type.cs",
                    "storage.type.groovy",
                    "storage.type.haskell",
                    "storage.type.objc",
                    "storage.type.ocaml",
                    "storage.type.php",
                    "string",
                    "string.interpolated",
                    "string.other",
                    "string.quoted",
                    "string.quoted.double",
                    "string.quoted.other",
                    "string.quoted.single",
                    "string.quoted.triple",
                    "string.regexp",
                    "string.unquoted",
                    "strong",
                    "support",
                    "support.class",
                    "support.constant",
                    "support.function",
                    "support.other",
                    "support.type",
                    "support.type.property-name",
                    "support.variable",
                    "variable",
                    "variable.language",
                    "variable.language",
                    "variable.name",
                    "variable.other",
                    "variable.other.readwrite",
                    "variable.other.readwrite.instance.ruby",
                    "variable.parameter",
                    "variable.parameter",
                    "variable.parameter.function.language.special"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "breadcrumbs.enabled": true,
    "window.zoomLevel": 0,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.minimap.enabled": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 17,
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.telemetry.enabled": false,
    "workbench.editor.highlightModifiedTabs": true,
    "editor.tabSize": 2,
    "eslint.alwaysShowStatus": true,
    "eslint.packageManager": "yarn",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "terminal.integrated.lineHeight": 1.2,
    "launch": {

    },
    "editor.letterSpacing": -0.2,
    "eslint.codeAction.disableRuleComment": {

    },
    "eslint.enable": true,
    "eslint.format.enable": true,
    "workbench.editor.showTabs": true,
    "git.enableCommitSigning": true,
    "editor.suggestLineHeight": 25,
    "ruby.rubocop.useBundler": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    }
}
