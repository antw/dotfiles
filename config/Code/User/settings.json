{
    "editor.fontFamily": "Jetbrains Mono",
    "editor.fontLigatures": true,
    "editor.rulers": [
        100
    ],
    "editor.tabCompletion": "on",
    "editor.acceptSuggestionOnEnter": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "workbench.iconTheme": "material-icon-theme",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.hovers.currentLine.over": "line",
    "newFile.defaultFileExtension": ".rb",
    "newFile.showPathRelativeTo": "project",
    "newFile.relativeTo": "file",
    "vim.useSystemClipboard": true,
    "eslint.validate": [
        "javascripta",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[elixir]": {
        "editor.formatOnSave": true,
    },
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.rulers": [100],
        "editor.tabSize": 4,
    },
    "[go]": {
        "editor.tabSize": 4
    },
    "go.testOnSave": true,
    "rust.show_warnings": false,
    // Disable italics.
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment keyword.codetag.notation",
                    "comment",
                    "comment.block",
                    "comment.block.documentation keyword",
                    "comment.block.documentation storage.type.class",
                    "comment.block.documentation variable",
                    "comment.block.documentation",
                    "comment.line",
                    "constant",
                    "constant.character",
                    "constant.character.escape",
                    "constant.numeric",
                    "constant.numeric.float",
                    "constant.numeric.hex",
                    "constant.numeric.integer",
                    "constant.numeric.octal",
                    "constant.other",
                    "constant.regexp",
                    "constant.rgb-value",
                    "emphasis",
                    "entity",
                    "entity.name",
                    "entity.name.class",
                    "entity.name.function",
                    "entity.name.method",
                    "entity.name.section",
                    "entity.name.selector",
                    "entity.name.tag",
                    "entity.name.type",
                    "entity.name.type",
                    "entity.name.variable.parameter",
                    "entity.other",
                    "entity.other.attribute-name",
                    "entity.other.attribute-name.pseudo-class.css",
                    "entity.other.inherited-class",
                    "invalid",
                    "invalid.deprecated",
                    "invalid.illegal",
                    "keyword",
                    "keyword.control",
                    "keyword.expressions-and-types.swift",
                    "keyword.operator",
                    "keyword.operator.arithmetic",
                    "keyword.operator.assignment",
                    "keyword.operator.logical",
                    "keyword.operator.new",
                    "keyword.other",
                    "keyword.other.this",
                    "keyword.primitive-datatypes.swift",
                    "keyword.type.cs",
                    "markup",
                    "markup.bold",
                    "markup.changed",
                    "markup.deleted",
                    "markup.heading",
                    "markup.inline.raw",
                    "markup.inserted",
                    "markup.italic",
                    "markup.list",
                    "markup.list.numbered",
                    "markup.list.unnumbered",
                    "markup.other",
                    "markup.quote",
                    "markup.raw",
                    "markup.underline",
                    "markup.underline.link",
                    "meta",
                    "meta.at-rule.function variable",
                    "meta.at-rule.mixin variable",
                    "meta.block",
                    "meta.cast",
                    "meta.class",
                    "meta.decorator variable.other.readwrite",
                    "meta.export variable.other.readwrite",
                    "meta.function",
                    "meta.function-call",
                    "meta.function.arguments variable.other.php",
                    "meta.import variable.other.readwrite",
                    "meta.object-binding-pattern-variable variable.object.property",
                    "meta.preprocessor",
                    "meta.protocol-list.objc",
                    "meta.return-type",
                    "meta.return-type.objc",
                    "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql",
                    "meta.selector",
                    "meta.tag",
                    "meta.type",
                    "meta.type.annotation",
                    "meta.variable.assignment.destructured.object.coffee variable",
                    "punctuation.definition.block.tag.jsdoc",
                    "punctuation.definition.string.begin",
                    "punctuation.definition.string.end",
                    "punctuation.separator",
                    "punctuation.separator.continuation",
                    "punctuation.terminator",
                    "source.go storage.type",
                    "storage",
                    "storage.class.std.rust",
                    "storage.modifier",
                    "storage.type",
                    "storage.type.attribute.swift",
                    "storage.type.c",
                    "storage.type.core.rust",
                    "storage.type.cs",
                    "storage.type.groovy",
                    "storage.type.haskell",
                    "storage.type.objc",
                    "storage.type.ocaml",
                    "storage.type.php",
                    "string",
                    "string.interpolated",
                    "string.other",
                    "string.quoted",
                    "string.quoted.double",
                    "string.quoted.other",
                    "string.quoted.single",
                    "string.quoted.triple",
                    "string.regexp",
                    "string.unquoted",
                    "strong",
                    "support",
                    "support.class",
                    "support.class.component",
                    "support.constant",
                    "support.function",
                    "support.other",
                    "support.type",
                    "support.type.property-name",
                    "support.variable",
                    "variable",
                    "variable.language",
                    "variable.language",
                    "variable.name",
                    "variable.other",
                    "variable.other.object",
                    "variable.other.readwrite",
                    "variable.other.readwrite.instance.ruby",
                    "variable.parameter",
                    "variable.parameter",
                    "variable.parameter.function.language.special"
                ],
                "settings": {
                    "fontStyle": ""
                }
            },
        ],
        "[Monokai Pro (Filter Spectrum)]": {
            "textMateRules": [
                {
                    "scope": [
                        "meta.example.jsdoc",
                        "source.embedded.ts"
                    ],
                    "settings": {
                        "foreground": "#69676C"
                    }
                },
            ]
        },
        "[*Material Theme Darker High Contrast]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "punctuation.definition.comment.python",
                        "punctuation.definition.comment.ruby",
                        "punctuation.definition.comment.scss",
                        "punctuation.definition.comment.ts",
                        "punctuation.definition.comment.tsx",
                        "string.quoted.docstring"
                    ],
                    "settings": {
                        "foreground": "#6c6c6c",
                        "fontStyle": ""
                    }
                },
            ]
        },
    },
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorCustomizations": {
        "[*Material Theme Darker High Contrast]": {
            "editor.lineHighlightBackground": "#2B2B2B",
        },
        "[Search Lights]": {
            "editor.lineHighlightBackground": "#313743"
        },
        "editorBracketHighlight.foreground1": "#ffd700",
        "editorBracketHighlight.foreground2": "#da70d6",
        "editorBracketHighlight.foreground3": "#87cefa",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000",
    },
    "breadcrumbs.enabled": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "editor.minimap.enabled": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 16,
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.telemetry.enabled": false,
    "workbench.editor.highlightModifiedTabs": true,
    "editor.tabSize": 2,
    "eslint.alwaysShowStatus": true,
    "eslint.packageManager": "yarn",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.stylelint": false
    },
    "terminal.integrated.lineHeight": 1.2,
    "editor.letterSpacing": -0.2,
    "eslint.codeAction.disableRuleComment": {

    },
    "eslint.enable": true,
    "eslint.format.enable": true,
    "workbench.editor.showTabs": true,
    "git.enableCommitSigning": true,
    "ruby.rubocop.useBundler": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "editor.fontSize": 16,
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 64,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": false
    },
    "go.lintTool":"golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "workbench.startupEditor": "welcomePage",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "css.validate": false,
    "html.format.contentUnformatted": "",
    "stylelint.config": {},
    "json.schemas": [

    ],
    "terminal.integrated.tabs.location": "left",
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.tree.indent": 12,
    "terminal.integrated.scrollback": 2000,
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "python.defaultInterpreterPath": "/Users/antw/.pyenv/shims/python",
    "ansible.ansibleLint.enabled": false,
    "ansible.ansible.useFullyQualifiedCollectionNames": true,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": false
    }
}
